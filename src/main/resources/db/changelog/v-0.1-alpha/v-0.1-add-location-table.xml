<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="1" author="nikitab">
        <createTable tableName="LOCATION">
            <column autoIncrement="true" name="ID" type="BIGINT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            <column name="LOCATION" type="VARCHAR(255)">
                <constraints nullable="false" unique="true"/>
            </column>
        </createTable>
    </changeSet>

    <changeSet id="2" author="nikitab">
        <insert tableName="LOCATION">
            <column name="LOCATION" value="Ramallah"/>
        </insert>
        <insert tableName="LOCATION">
            <column name="LOCATION" value="Beit-El"/>
        </insert>
        <insert tableName="LOCATION">
            <column name="LOCATION" value="Dura"/>
        </insert>
        <insert tableName="LOCATION">
            <column name="LOCATION" value="Duma"/>
        </insert>
    </changeSet>

</databaseChangeLog>